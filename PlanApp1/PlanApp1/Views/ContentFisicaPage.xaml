<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PlanApp1.Views.ContentFisicaPage">
    <ContentPage.Content>
      <StackLayout>
            <Label
                HorizontalOptions="Center"
                FontAttributes="Bold"
                FontSize="Large"
                Text="Lista de Docentes">

            </Label>
            <ListView x:Name="XamlListViewCatalogoProfesores"
              ItemsSource="{Binding ObservableCollectionFisicaProfesores}"
              ItemTapped="RedirigirPagina">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Nombre}"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>

            



        </StackLayout>
    </ContentPage.Content>
</ContentPage>